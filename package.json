{
  "name": "aftership",
  "description": "node.js SDK for AfterShip API",
  "version": "6.1.0",
  "homepage": "https://github.com/aftership/aftership-sdk-nodejs",
  "author": {
    "name": "AfterShip",
    "email": "support@aftership.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aftership/aftership-sdk-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/aftership/aftership-sdk-nodejs/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist ./coverage",
    "lint": "tslint --project .",
    "test": "jest",
    "coverage": "jest && codecov -t ${CODECOV_TOKEN} -F aftership"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.0.0",
    "axios-mock-adapter": "^1.18.2",
    "codecov": "^3.7.2",
    "debug": "^4.1.1",
    "jest": "^26.1.0",
    "pre-commit": "^1.2.2",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "aftership"
  ],
  "license": "MIT",
  "pre-commit": [
    "lint"
  ],
  "private": false,
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
