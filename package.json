{
  "name": "aftership",
  "description": "node.js SDK for AfterShip API",
  "version": "5.4.5",
  "homepage": "https://github.com/aftership/aftership-sdk-nodejs",
  "author": {
    "name": "AfterShip",
    "email": "support@aftership.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aftership/aftership-sdk-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/aftership/aftership-sdk-nodejs/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist ./coverage ./.nyc_output",
    "lint": "tslint --project .",
    "test": "nyc mocha --config .mocharc.json src/*.spec.js test/integration/*.js",
    "coverage": "codecov -F eventsapi --root=$PWD"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-aftership": "^1.10.1",
    "grunt": "^0.4.5",
    "grunt-eslint": "^17.3.1",
    "grunt-mocha-istanbul": "^3.0.1",
    "http-proxy": "^1.12.0",
    "istanbul": "^0.4.2",
    "load-grunt-config": "^0.19.1",
    "mocha": "^2.3.4",
    "pre-commit": "^1.1.2",
    "proxyquire": "^2.0.1",
    "sinon": "^1.17.2",
    "codecov": "^3.0.4",
    "nyc": "^14.1.1",
    "shx": "^0.3.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  },
  "keywords": [
    "aftership"
  ],
  "license": "MIT",
  "pre-commit": [
    "lint"
  ],
  "private": false
}